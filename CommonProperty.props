<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup>
    <IncludePath>$(SolutionDir)Include;$(SolutionDir)3rdParty\opencv\include;$(SolutionDir)3rdParty\rapidjson;$(SolutionDir)3rdParty\libcurl\include;$(SolutionDir)3rdParty\tinyxml2\include;$(IncludePath)</IncludePath>
